<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//OTD Mapper 3.0//EN" "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qfl.trading.mapper.StringMapper">
    <select id="getData" resultType="Data" parameterType="String" databaseId="mysql">
        select sqlName from files where fileID=#{database}
    </select>

    <select id="getDatabase" resultType="String" parameterType="int" databaseId="mysql">
        select * from data
    </select>
</mapper>